<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="50.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fr.vinvin129.budgetmanager.ihm.views.controllers.dashboard.NavbarController">
    <opaqueInsets>
        <Insets />
    </opaqueInsets>
    <BorderPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <left>
          <Button fx:id="previousButton" onAction="#previous" mnemonicParsing="false" text="Précédent" BorderPane.alignment="CENTER" />
        </left>
        <right>
          <Button fx:id="nextButton" onAction="#next" mnemonicParsing="false" text="Suivant" BorderPane.alignment="CENTER" />
        </right>
        <center>
          <Label fx:id="monthLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="25.0" prefWidth="202.0" text="Under know" textAlignment="CENTER" wrapText="true" BorderPane.alignment="CENTER">
              <font>
                  <Font name="Comic Sans MS" size="18.0" />
              </font>
          </Label>
        </center>
        <opaqueInsets>
          <Insets />
        </opaqueInsets>
    </BorderPane>
   <padding>
      <Insets bottom="10.0" left="20.0" right="20.0" top="10.0" />
   </padding>
</AnchorPane>
